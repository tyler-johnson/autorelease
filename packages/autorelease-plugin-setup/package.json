{
  "name": "autorelease-plugin-setup",
  "version": "0.0.0",
  "description": "A plugin providing the Autorelease setup CLI tool.",
  "author": "Tyler Johnson <tyler@tylerjohnson.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyler-johnson/autorelease.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -f index.js",
    "build-lib": "rollup src/index.js -c -o index.js",
    "build": "npm-run-all clean build-lib",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "autorelease-pipeline": "*",
    "autorelease-task-npm-registry": "*",
    "autorelease-test-utils": "*",
    "chalk": "^1.1.3",
    "es6-promisify": "^5.0.0",
    "get-npm-token": "^1.0.1",
    "inquirer": "^1.2.2",
    "keytar": "^3.0.2",
    "lodash": "^4.16.6",
    "nerf-dart": "^1.0.0",
    "progress": "^1.1.8",
    "shell-escape": "^0.2.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.5",
    "shx": "^0.2.2"
  },
  "files": [
    "index.js"
  ]
}
