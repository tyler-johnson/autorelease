{
  "name": "autorelease-plugin-lerna",
  "version": "0.0.0",
  "description": "An Autorelease plugin to release Lerna packages.",
  "author": "Tyler Johnson <tyler@tylerjohnson.me>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyler-johnson/autorelease.git"
  },
  "main": "index.js",
  "scripts": {
    "clean": "shx rm -f index.js",
    "build-lib": "rollup src/index.js -c -o index.js",
    "build": "npm-run-all clean build-lib",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "autorelease-pipeline": "*",
    "autorelease-task-configure-npm": "*",
    "autorelease-task-fetch-commits": "*",
    "autorelease-task-fetch-latest": "*",
    "autorelease-task-generate-changelog": "*",
    "autorelease-task-npm-publish": "*",
    "autorelease-task-prep-publish": "*",
    "autorelease-task-resolve-version": "*",
    "lodash": "^4.16.4",
    "progress": "^1.1.8"
  },
  "peerDependencies": {
    "lerna": "^2.0.0-beta.30"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "rollup": "^0.41.5",
    "shx": "^0.2.2"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "autorelease",
    "autorelease-plugin"
  ]
}
